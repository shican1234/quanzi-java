<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="io.renren.common.modules.customer.dao.CustomerDao">


    <!--  删除所有的客服配置记录  -->
    <delete id="deleteCustomerAll">
        truncate table tb_customer
    </delete>

    <!-- 查询后台配置的客服联系方式 -->
    <select id="getCustomerAll" resultType="io.renren.common.modules.customer.entity.CustomerEntity">
        select

            *

        from

            tb_customer

    </select>



</mapper>